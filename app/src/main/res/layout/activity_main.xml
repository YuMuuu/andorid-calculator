<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <Button
            android:text="="
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="503dp"
            tools:layout_editor_absoluteX="307dp" android:id="@+id/button19"/>
    <Button
            android:text="00"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="503dp"
            tools:layout_editor_absoluteX="115dp" android:id="@+id/button20"/>
    <Button
            android:text="0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="503dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/button21"/>
    <Button
            android:text="."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="503dp"
            tools:layout_editor_absoluteX="210dp" android:id="@+id/button22"/>
    <Button
            android:text="2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="439dp"
            tools:layout_editor_absoluteX="115dp" android:id="@+id/button11"/>
    <Button
            android:text="-"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="376dp"
            tools:layout_editor_absoluteX="307dp" android:id="@+id/button12"/>
    <Button
            android:text="+"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="439dp"
            tools:layout_editor_absoluteX="307dp" android:id="@+id/button13"/>
    <Button
            android:text="3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="439dp"
            tools:layout_editor_absoluteX="210dp" android:id="@+id/button14"/>
    <Button
            android:text="4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="376dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/button15"/>
    <Button
            android:text="5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="376dp"
            tools:layout_editor_absoluteX="115dp" android:id="@+id/button16"/>
    <Button
            android:text="6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="376dp"
            tools:layout_editor_absoluteX="210dp" android:id="@+id/button17"/>
    <Button
            android:text="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="439dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/button18"/>
    <Button
            android:text="*"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="315dp"
            tools:layout_editor_absoluteX="307dp" android:id="@+id/button7"/>
    <Button
            android:text="7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="315dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/button8"/>
    <Button
            android:text="8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="315dp"
            tools:layout_editor_absoluteX="115dp" android:id="@+id/button9"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="315dp"
            tools:layout_editor_absoluteX="210dp" android:id="@+id/button10" android:text="9"/>
    <TextView
            android:id="@+id/message_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="100sp" app:layout_constraintHorizontal_bias="0.881"
            app:layout_constraintVertical_bias="0.065"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="76dp"
            android:text="Increment" android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="Decrement"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button2" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="76dp"
            app:layout_constraintEnd_toStartOf="@+id/button" android:layout_marginEnd="8dp"
            android:layout_marginBottom="24dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="AC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="255dp"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/button3"/>
    <Button
            android:text="+/-"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="255dp"
            tools:layout_editor_absoluteX="115dp" android:id="@+id/button4"/>
    <Button
            android:text="%"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="255dp"
            tools:layout_editor_absoluteX="210dp" android:id="@+id/button5"/>
    <Button
            android:text="/"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="255dp"
            tools:layout_editor_absoluteX="307dp" android:id="@+id/button6"/>

</android.support.constraint.ConstraintLayout>